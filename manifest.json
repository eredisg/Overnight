{
    "manifest_version": 2,
    "name": "Overnight",
    "version": "1.0",
  
    "description": "Auto-refresh after a certain interval & notifies you if page changes.",
  
    "icons": {
        "48": "icons/tent-sleep-icon.png"
    },
  
    "content_scripts": [
      {
          "matches": ["<all_urls>"],
          "js": ["main.js"]
      }
    ]
  
  }